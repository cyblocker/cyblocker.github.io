ebsites.net/hackathons/761/projects/50431\n\n","_N$overflow":3},{"__type__":"cc.Widget","node":{"__id__":16},"_alignFlags":40,"_originalWidth":2336}]},{"__type__":"cc.Node","_name":"810160683","_parent":{"__id__":14},"_contentSize":{"__type__":"cc.Size","width":240,"height":160},"_scaleX":0.9,"_scaleY":0.9,"_position":{"__type__":"cc.Vec2","y":-1120},"_components":[{"__type__":"cc.Sprite","node":{"__id__":17},"_spriteFrame":{"__uuid__":"0235f8ff-e21e-4ffc-b591-be5103ca3fec"},"_sizeMode":0}]},{"__type__":"cc.Node","_name":"New Button","_parent":{"__id__":6},"_contentSize":{"__type__":"cc.Size","width":100,"height":40},"_children":[{"__id__":19}],"_position":{"__type__":"cc.Vec2","y":-193},"_components":[{"__type__":"cc.Sprite","node":{"__id__":18},"_spriteFrame":{"__uuid__":"f0048c10-f03e-4c97-b9d3-3506e1d58952"},"_type":1,"_sizeMode":0},{"__type__":"cc.Button","node":{"__id__":18},"transition":2,"pressedColor":{"__type__":"cc.Color","r":255,"g":255,"b":255},"pressedSprite":{"__uuid__":"e9ec654c-97a2-4787-9325-e6a10375219a"},"hoverSprite":{"__uuid__":"e9ec654c-97a2-4787-9325-e6a10375219a"},"clickEvents":[{"__type__":"cc.ClickEvent","target":{"__id__":18},"component":"Start","handler":"callback"}],"_N$normalColor":{"__type__":"cc.Color","r":255,"g":255,"b":255},"_N$disabledColor":{"__type__":"cc.Color","r":255,"g":255,"b":255},"_N$normalSprite":{"__uuid__":"f0048c10-f03e-4c97-b9d3-3506e1d58952"},"_N$disabledSprite":{"__uuid__":"29158224-f8dd-4661-a796-1ffab537140e"},"_N$target":{"__id__":18}},{"__type__":"cc.Widget","node":{"__id__":18},"_alignFlags":16},{"__type__":"b214b5Ev2hLG7ig8UZJ2JxU","node":{"__id__":18}}]},{"__type__":"cc.Node","_name":"Label","_color":{"__type__":"cc.Color"},"_parent":{"__id__":18},"_contentSize":{"__type__":"cc.Size","width":100,"height":40},"_components":[{"__type__":"cc.Label","node":{"__id__":19},"_useOriginalSize":false,"_actualFontSize":20,"_fontSize":20,"_enableWrapText":false,"_N$string":"再来一次","_N$horizontalAlign":1,"_N$verticalAlig